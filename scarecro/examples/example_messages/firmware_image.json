        {
            "field_name": "id",
            "display_name": "ID",
            "data_type": "string",
            "kind": "reported",
            "description": "id of sensor"
        },
        {
            "field_name": "time",
            "display_name": "Time",
            "data_type": "date",
            "kind": "reported",
            "description": "reading time"
        },
        {
            "field_name": "file_name",
            "display_name": "File Name",
            "data_type": "string",
            "kind": "reported",
            "description": "Name of file to download"
        },
        {
            "field_name": "cloud_path",
            "display_name": "Cloud Path",
            "data_type": "string",
            "kind": "reported",
            "description": "Location of the file in the cloud (including filename)"
